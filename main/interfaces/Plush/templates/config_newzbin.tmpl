<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
	
	<title>SABnzbd+ $version - Queued: $mbleft MB</title>
	<link rel="shortcut icon" type="image/ico" href="../../static/images/plush-default/sabnzbdplus.ico"/>
	
	<script type="text/javascript" src="../../static/javascripts/jquery.js?$version"></script>
	<script type="text/javascript" src="../../static/javascripts/jquery.corner.js?$version"></script>
	
	<link rel="stylesheet" type="text/css" href="../../static/stylesheets/plush-default.css?$version"/>
	<!--#if $color_scheme and $color_scheme != "plush-default.css" and $color_scheme != "plush_default.css" and $color_scheme != "default.css" and $color_scheme != "modern.css" and $color_scheme != "Modern.css"#-->
	<link rel="stylesheet" type="text/css" href="../../static/stylesheets/colorschemes/$color_scheme?$version"/>
	<!--#end if#-->
	
	<!--#raw#-->
	<script language="javascript">
		$(document).ready(function(){
			$('.config_menu').corner("round tl bl");
			$('#config_container').corner("round");
			$('#help').corner("round");
			$('#save').corner("round");
		});
	</script>
	<!--#end raw#-->
	
	<style>
	<!--
		body {background: #ddd;}
	-->
	</style>
	
</head>
<body>
	
	<div id="config_go_back"><div><a href="../../">go&nbsp;back</a></div></div>
	
	<div id="config_nav">
		<ul>
			<li><a class="config_menu" id="config_nav_connections" href="../../connections">Connections</a></li>
			<li><a class="config_menu" id="config_nav_general" href="../general">General</a></li>
			<li><a class="config_menu" id="config_nav_directories" href="../directories">Folders</a></li>
			<li><a class="config_menu" id="config_nav_switches" href="../switches">Switches</a></li>
			<li><a class="config_menu" id="config_nav_server" href="../server">Servers</a></li>
			<li><a class="config_menu" id="config_nav_categories" href="../categories">Categories</a></li>
			<li><a class="config_menu" id="config_nav_scheduling" href="../scheduling">Scheduling</a></li>
			<li><a class="config_menu" id="config_nav_rss" href="../rss">RSS</a></li>
			<li><a class="config_menu" id="config_nav_email" href="../email">Email</a></li>
			<li><a class="config_menu nav_active" id="config_nav_newzbin" href="../newzbin">Newzbin</a></li>
		</ul>
	</div>
	
	<div id="config_container" class="config_margin">
		<div id="config_content">
			
<!--************************************
			PAGE CONTENT IS BELOW
	************************************ -->
	
			<small>If you have an account at <a href="https://www.newzbin.com" target="_blakn">www.newzbin.com</a>, you can enter your account info here; this will unlock extra functionality.</small><br/><br/>
	
			<form action="saveNewzbin" method="post" name="fullform">
			<div class="EntryBlock">
			<fieldset class="EntryFieldSet">
				
				<legend id="configbox_newzbin_account_info">Newzbin&nbsp;Account&nbsp;Info</legend>
				
				<strong style="color:white;background-color:green;">&nbsp;Optional&nbsp;</strong><br/><br/>
				
				<strong>Username:</strong><br/>
				<small>Set your Newzbin username here.</small><br/>
				<input type="text" name="username_newzbin" value="$username_newzbin"/>
				
				<br/><br/>
				<strong>Password:</strong><br/>
				<small>Set your Newzbin password here.</small><br/>
				<input type="password" name="password_newzbin" value="$password_newzbin"/>
				
			</fieldset>
			</div>
			
			<div class="EntryBlock">
			<fieldset class="EntryFieldSet">
				
				<legend id="configbox_bookmark_processing">Bookmark&nbsp;Processing</legend>
				
				<input type="checkbox" name="newzbin_bookmarks" value="1" <!--#if $newzbin_bookmarks > 0 then "checked=1" else ""#--> />
				<strong>Auto Fetch Bookmarks</strong><br/>
				<small>Automatically fetches jobs from your bookmarks.</small><br/>
				
				<br/>
				&raquo; <a href="getBookmarks">Get Bookmarks Now</a>
				
				<br/>
				<!--#if $bookmarks_list#-->
				&raquo; <a href="hideBookmarks">Hide Processed Bookmarks</a>
				<!--#else#-->
				&raquo; <a href="showBookmarks">Show Processed Bookmarks</a>
				<!--#end if#-->
				
				<br/><br/>
				<input type="checkbox" name="newzbin_unbookmark" value="1" <!--#if $newzbin_unbookmark > 0 then "checked=1" else ""#--> />
				<strong>Un-bookmark If Download Complete</strong><br/>
				<small>Remove from bookmark list, when download is complete.</small><br/>
				
				<br/>
				<strong>Checking Interval:</strong><br/>
				<small>In minutes.</small><br/>
				<input type="text" name="bookmark_rate" value="$bookmark_rate"/><small>&nbsp;(at&nbsp;least&nbsp;15&nbsp;min)</small>
				
			</fieldset>
			</div>
			<!--#if $bookmarks_list#-->
			<fieldset class="EntryFieldSet">
				
				<legend id="configbox_processed_bookmarks">Processed Bookmarks</legend>
				
				<!--#for $msgid in $bookmarks_list#-->
				<a href="https://www.newzbin.com/browse/post/$msgid/" target="_blank">$msgid</a><br/>
				<!--#end for#-->
				
			</fieldset>
			<!--#end if#-->
			
<!--************************************
			PAGE CONTENT IS ABOVE
	************************************ -->
			
		</div>
	</div>
	
	<div class="left_links config_margin">
		<a id="help" target="_blank" href="$helpuri/Configure+Newzbin">Newzbin Help</a>
	</div>
	
	<div class="right_links">
		<a id="save" onclick="document.fullform.submit();">Save Changes</a>
	</div>
	
	</form>
	
</body>
</html>
