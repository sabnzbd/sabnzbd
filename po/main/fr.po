msgid "Add Server"
msgstr "Ajouter un serveur"

#: sabnzbd/skintext.py:443
msgid "Host"
msgstr "Hôte"

#: sabnzbd/skintext.py:444
msgid "Port"
msgstr "Port"

#: sabnzbd/skintext.py:445
msgid "Username"
msgstr "Nom d'utilisateur"

#: sabnzbd/skintext.py:446
msgid "Password"
msgstr "Mot de passe"

